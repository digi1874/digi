{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/ink-docstrap/template",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "readme": "README.md"
  },
  "templates": {
    "systemName"            : "digi",
    "includeDate"           : false,
    "navType"               : "inline",
    "theme"                 : "slate",
    "linenums"              : true,
    "outputSourceFiles"     : true,
    "syntaxTheme"           : "dark"
  }
}